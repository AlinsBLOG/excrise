<import src="../template/template.wxml"/>

<view class='searchBox'>
  <input 
    placeholder='搜索' 
    placeholder-class='holderClass'
    class='search'
    bindconfirm='confirm'></input>
</view>

<scroll-view scroll-y="true" bindscrolltolower="searchScrollLower" class='mrt30'>
  <block wx:for='{{cardList}}' wx:key='index'>
    <template is="hot" data='{{...item,globalImage}}' />
  </block>
  <view wx:if='{{!request}}' class='loadOver'>加载完毕</view>
</scroll-view>


