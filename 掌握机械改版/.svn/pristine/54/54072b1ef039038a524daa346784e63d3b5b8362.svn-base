page{
    background-color: #f4f4f4;
    height: 100%;
    overflow-y:hidden;
}
.body{
    height: 100%;
}
.img_bg{
    width: 100%;
    height: 385rpx;
}
.a{
    top: 0;
    left: 0;
}
.b{
    top: 92rpx;
    left: 0;
}
.c{
    top: 182rpx;
    left: 0;
}
.d{
    top: 260rpx;
    left: 0;
}
/*search*/
.search{
	width: 715rpx;
	height: 54rpx;
	padding: 20rpx;
	z-index: 2;
}
.search image{
	width: 32rpx;
	height: 32rpx;
	left: 32%;
	top:33rpx;
    /* margin-left:-91px; */
    z-index: 3;
}
.search input{
	/* background-color: #e6e6e6; */
	border: 1px solid #e6e6e6;
    border-radius:100px;
    background-color: #fff;
}
/* 商家信息 */
.supply_company {
    width: 100%;
    height: 120rpx;
    padding: 0 0 0 140rpx;
    padding-right: 25%;
}
.company_logo {
    width: 80rpx;
    height: 80rpx;
    top: 50%;
    left: 32rpx;
    transform: translateY(-50%);
}
.company_nav{
    top: 50%;
    left: 138rpx;
    transform: translateY(-50%);
}
.supply_title {
    font-weight: 500;
}
.collect {
    color: #333333;
    top: 60rpx;
    right: 40rpx;
}
.collect image{
    width:30rpx;
    height:30rpx;
    top: -30rpx;
    left: 50%;
    transform: translateX(-50%)
}
.content{
    width: 100%;
    min-height: 76rpx;
    overflow : hidden;
    padding: 0 20rpx 0 20rpx;
    box-sizing: border-box;
    line-height: 38rpx;
    text-indent: 2em;
}
.fold{
    width: 100%;
    height: 60rpx;
}
.text_hidden{
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.fold image{
    width:36rpx;
    height:21rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%)
}
  /*banner*/
.swiper,swiper{
	width: 100%;
	height: 375rpx;
	position: relative;
}
.dots{
	position: absolute;
	right: 20rpx;
	bottom: 15rpx;
	overflow: hidden;
}
.dot{
	float: left;
	width: 15rpx;
	height: 15rpx;
	border: 1px solid #fff;
	border-radius: 50%;
	margin-left: 15rpx;
}
.bannerAct{
	border: 1px solid #fd5f2a;
	background-color: #fd5f2a;
}
/* 热销 */
.hotsale_title {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    color:#333333;
    background: #ffffff;
    padding-left: 40rpx;
}
.hotsale_title .border{
    top: 50%;
    left: 20rpx;
    transform: translateY(-50%);
}
.supply_goods {
    width: 100%;
    height: 325rpx;
    white-space: nowrap;
  }
  .supply_goodsbox {
    width: 180rpx;
    height: 325rpx;
    display: inline-block;
    margin-right: 30rpx;
  }
  .supply_goodsimage {
    width: 180rpx;
    height: 180rpx;
  }
  .supply_goodsname {
    height: 60rpx;
    white-space: pre-wrap;
  }
/* 排序 */
.options{
	width: 100%;
    height: 90rpx;
    border-bottom: 1px solid #eee;
}
.select{
	flex:1;
	height: 80rpx;
	line-height: 90rpx;
	text-align: center;
	color: #666;
	font-size: 14px;
}
.selectAct{
	color: #fd5f2a;
}
.sort{
	position: absolute;
	width: 16rpx;
	height: 16rpx;
	background-color: #fff;
	box-sizing: border-box;
	left: 50%;
	margin-left: 16px;
	border: 8rpx solid transparent;
}
.asc{
	bottom: 50%;
	border-bottom: 8rpx solid #666;
}
.desc{
	top: 60%;
	border-top: 8rpx solid #666;
}
.asc_active{
	border-bottom: 8rpx solid #fd5f2a;
}
.desc_active{
	border-top: 8rpx solid #fd5f2a;
}
/* 蒙版 */
.mask{
    width: 100%;
    height: 100%;
    z-index: 10;
    background:rgba(0,0,0,0.30);
}
.mask2{
    width: 100%;
    height: 184rpx;
    z-index: 9;
    background:transparent;
}
.mask3{
    width: 100%;
    height: 184rpx;
    background:transparent;
}
/* 筛选 */
.filtrate{
    width: 26%;
	height: 60rpx;
	line-height: 60rpx;
    text-align: center;
    background:#f5f5f5;
    border-radius:2px;
	color: #666;
    font-size: 12px;
    padding-top: 7rpx;
    margin: 10rpx;
}
.filtrate_img{
    width: 38rpx;
    height: 36rpx;
    right: 5%;
    top:50%;
    transform: translateY(-50%);
}
/* 子列表 */
.ulLis{
    width:100%;
    height:92rpx;
    line-height: 92rpx;
    padding-left: 30rpx;
    z-index: 99;
    border-bottom: 1px solid #eee;
}
.goodsType{
    width:100%;
    height: 600rpx;
}
.goodsType .ulLis{
    /* width:30%; */
    width: 100%;
}
.nextType{
    width:70%;
    height: 600rpx;
    top: 0;
    right: 0;
}
/* 格子布局 */
/* 循环父盒子宽度 */
.waterfall_box{
    width: 744rpx;
    padding-bottom:10rpx; 
}
/* 循环子盒子宽高 */
.waterfall_goods {
    width: 366rpx;
    height: 584rpx;
    margin-right: 7rpx;
    padding-bottom: 10rpx;
    float: left;
  }
/* 分享，找客服 */
.share {
    background: rgba(0, 0, 0, 0.7);
    width: 96rpx;
    height: 96rpx;
    bottom: 25%;
    right: 2%;
    border-radius: 100%;
    box-shadow: 0 2rpx 2rpx rgba(0, 0, 0, 0.7);
    color: #bbb;
    z-index:1000;
  }
  .service {
    background: rgba(255, 255, 255, 1);
    width: 96rpx;
    height: 96rpx;
    bottom: 15%;
    right: 2%;
    border-radius: 100%;
    box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.3);
    color: #1aad19;
    z-index:1000;
    
  }
  .share image {
    width: 46rpx;
    height: 46rpx;
    top: 16rpx;
    left: 50%;
    transform: translateX(-50%);
  }
  .service image {
    width: 58rpx;
    height: 46rpx;
    top: 16rpx;
    left: 50%;
    transform: translateX(-50%);
  }
  .share text,
  .service text {
    width: 80rpx;
    bottom: 10rpx;
    left: 50%;
    transform: translateX(-50%);
  }
  .service_green {
    width: 476rpx;
    height: 80rpx;
    bottom: 15%;
    right: 17%;
    color: #fff;
    opacity:0
  }
  .service_green image{
    width: 100%;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
  }
  .service_green text {
    width: 462rpx;
    height: 80rpx;
    line-height: 80rpx;
    top: 0;
    left: 0;
  }
/* 回到顶部 */
.returnTop{
	bottom: 7%;
	right: 3%;
	width: 68rpx;
	height: 68rpx;
	border: 1px solid #ccc;
    border-radius: 50%;
    box-shadow: 0 2rpx 2rpx rgba(0, 0, 0, 0.7);
	text-align: center;
	opacity: .8;
}
.return_img{
	width: 40rpx;
	height: 20rpx;
	margin: 14rpx auto 0;
}
.f8{
	font-size: 8px;
}
.orange{color: #fd5f2a;}