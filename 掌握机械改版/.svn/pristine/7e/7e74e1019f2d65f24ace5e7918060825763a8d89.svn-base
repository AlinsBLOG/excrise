}

.textoh {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.textoh2 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.text_hidden {
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

/*wrap*/

.wrap {
  word-wrap: normal;
  word-spacing: normal;
