  height: 112rpx;
  line-height: 112rpx;
  background: #fff;
  color: #666;
}

.waterfall_box {
  width: 744rpx;
  padding-bottom: 10rpx;
}

/* 循环子盒子宽高 */

.waterfall_goods {
  width: 366rpx;
  height: 584rpx;
  margin-right: 7rpx;
  padding-bottom: 10rpx;
  float: left;
}


