/* 订单为空 */
.nullCont {
  height: 706rpx;
  width: 100%;
  background: #fafafa;
  text-align: center;
}

.whereAddr {
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  font-size: 14px;
  color: #999;
}

.whereAddr image {
  width: 265px;
  height: 143px;
  margin-bottom: 32rpx;
}

.noOrderMsg {
  color: #797979;
}

/* 猜你喜欢 */

