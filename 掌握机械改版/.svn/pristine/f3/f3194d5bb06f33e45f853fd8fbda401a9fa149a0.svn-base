page {
  background-color: #f4f4f4;
  height: 100%;
  overflow-y: hidden;
}

.body {
  height: 100%;
}

.pf_view {
  width: 100%;
  height: 260rpx;
  left: 0rpx;
}

.a {
  top: 0;
  left: 0;
}

.b {
  top: 119rpx;
  left: 0;
}

.c {
  top: 170rpx;
  left: 0;
}

.d {
  top: 262rpx;
  left: 0;
}

/*search*/

.search {
  width: 100%;
  height: 54rpx;
  padding: 20rpx;
  z-index: 5;
}

.search .search_img {
  width: 32rpx;
  height: 32rpx;
  left: 36rpx;
  top: 36rpx;
}

.search .empty {
  width: 30rpx;
  height: 30rpx;
  left: 580rpx;
  top: 35rpx;
  z-index: 6;
}

.search input {
  width: 560rpx;
  height: 60rpx;
  background-color: #f6f6f6;
  border-radius: 100px;
  padding-left: 60rpx;
}

.button {
  width: 90rpx;
  height: 62rpx;
  padding: 0;
  line-height: 62rpx;
  border: 1rpx solid #ddd;
  background-color: #f0f0f0;
  right: 50rpx;
  top: 50%;
  transform: translateY(-50%);
  border-radius: 3px;
}

.search .wechat {
  width: 40rpx;
  height: 40rpx;
  right: 9%;
  top: 50%;
  transform: translateY(-50%);
}

.search .wechat image {
  width: 38rpx;
  height: 36rpx;
  right: 9%;
  top: 50%;
  transform: translateY(-50%);
}

.wechat::after {
  border: none;
}

/* 搜索结果 */

.rec_goodsbox {
  width: 100%;
  padding: 20rpx 10rpx;
  border-bottom: 1px solid #eee;
  display: flex;
  align-items: center;
}

.rec_image {
  width: 250rpx;
  height: 250rpx;
  flex: 2;
  margin-right: 40rpx;
}

.rec_goodsinfo {
  flex: 3;
}

.rec_goodsname {
  height: 72rpx;
  line-height: 36rpx;
}

.rec_company {
  width: 100%;
  height: 32rpx;
  line-height: 32rpx;
  bottom: 0rpx;
  left: 0rpx;
}

.rec_goodsprice {
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 25rpx;
}

/* 排序 */

.options {
  width: 100%;
  height: 90rpx;
  border-bottom: 1px solid #eee;
  z-index: 5;
}

.select {
  flex: 1;
  height: 80rpx;
  line-height: 90rpx;
  text-align: center;
  color: #666;
  font-size: 14px;
}

.selectAct {
  color: #fd5f2a;
}

.sort {
  position: absolute;
  width: 16rpx;
  height: 16rpx;
  background-color: #fff;
  box-sizing: border-box;
  left: 50%;
  margin-left: 16px;
  border: 8rpx solid transparent;
}

.asc {
  bottom: 50%;
  border-bottom: 8rpx solid #666;
}

.desc {
  top: 60%;
  border-top: 8rpx solid #666;
}

.asc_active {
  border-bottom: 8rpx solid #fd5f2a;
}

.desc_active {
  border-top: 8rpx solid #fd5f2a;
}

/* 筛选 */

.filtrate {
  width: 26%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background: #f5f5f5;
  border-radius: 2px;
  color: #666;
  font-size: 12px;
  padding-top: 7rpx;
  margin: 10rpx;
}

.filtrate_img {
  width: 38rpx;
  height: 36rpx;
  right: 5%;
  top: 50%;
  transform: translateY(-50%);
}

.orange {
  color: #fd5f2a;
}

/* 搜索结果无 */

.searchNull {
  width: 100%;
  height: 540rpx;
  box-sizing: border-box;
}

.searchNull>image {
  width: 178rpx;
  height: 166rpx;
  top: 162rpx;
  left: 50%;
  transform: translateX(-50%);
}

.searchNull>text {
  top: 340rpx;
  left: 50%;
  transform: translateX(-50%);
}

.searchNull_button>image {
  width: 46rpx;
  height: 36rpx;
  top: 50%;
  left: 30%;
  transform: translateY(-50%);
}

.searchNull_button {
  background: #23b123;
  border-radius: 4px;
  width: 692rpx;
  height: 80rpx;
  top: 446rpx;
  left: 50%;
  transform: translateX(-50%);
  margin: 0;
  padding: 0;
}

.searchNull_button::after {
  border: none;
}

.searchNull_button>text {
  top: 0;
  left: 41%;
  line-height: 80rpx;
}

/* 格子布局 */

/* 循环父盒子宽度 */

.waterfall_box {
  width: 744rpx;
  padding-bottom: 10rpx;
}

/* 循环子盒子宽高 */

.waterfall_goods {
  width: 366rpx;
  height: 584rpx;
  margin-right: 7rpx;
  padding-bottom: 10rpx;
  float: left;
}

/* 蒙版 */
.mask {
  width: 100%;
  height: 100%;
  z-index: 10;
  background: rgba(0, 0, 0, 0.30);
}

.mask2 {
  width: 100%;
  height: 260rpx;
  z-index: 9;
  background: transparent;
}

.mask3 {
  width: 100%;
  /* background:transparent; */
}

.category-flex {
  display: flex;
}

/* 子列表 */

.ulLis {
  width: 100%;
  height: 92rpx;
  line-height: 92rpx;
  padding-left: 30rpx;
  z-index: 99;
  border-bottom: 1px solid #eee;
}

.goodsType {
  flex: 1;
  display: inline-block;
  height: 600rpx;
}

.goodsType .ulLis {
  width: 100%;
}

.nextType {
  flex: 1;
  height: 600rpx;
  display: inline-block;
}

.scroll,.waterfal{
  padding-top: 260rpx;
}

/* 回到顶部 */

.returnTop {
  bottom: 70rpx;
  right: 3%;
  width: 68rpx;
  height: 68rpx;
  border: 1px solid #ccc;
  border-radius: 50%;
  box-shadow: 0 2rpx 2rpx rgba(0, 0, 0, 0.7);
  text-align: center;
  opacity: 0.8;
}

.return_img {
  width: 40rpx;
  height: 20rpx;
  margin: 14rpx auto 0;
}

.f8 {
  font-size: 8px;
}

/*清除浮动代码*/

.clearfloat:after {
  display: block;
  clear: both;
  content: "";
  visibility: hidden;
  height: 0;
}

.clearfloat {
  zoom: 1;
}
