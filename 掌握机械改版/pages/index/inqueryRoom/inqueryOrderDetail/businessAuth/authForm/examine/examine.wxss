page {
  background: #f5f5f5;
}

.container {
  text-align: center;
}

.icon {
  width: 92rpx;
  height: 92rpx;
  margin: 100rpx 0 20rpx 0;
}

.content {
  font-size: 40rpx;
  color: #323232;
  font-weight: 600;
}

.title {
  font-size: 28rpx;
  font-weight: 600;
  color: #646464;
}

.dec {
  font-size: 12px;
  color: #999;
  font-weight: 600;
  line-height: 34rpx;
}

.phone {
  left: 50%;
  bottom: 92rpx;
  transform: translateX(-50%);
  font-size: 28rpx;
  font-weight: 600;
  color: #323232;
}
.time {
  font-size: 24rpx;
  color: #646464;
  left: 50%;
  bottom: 50rpx;
  transform: translateX(-50%);
  font-weight: 600;
  min-width: 400rpx;
}
.backTo {
  left: 50%;
  transform: translateX(-50%);
  position: fixed;
  width:160rpx;
  font-size: 30rpx;
  bottom: 160rpx;
  /* color: #fff; */
  padding: 20rpx;
}
