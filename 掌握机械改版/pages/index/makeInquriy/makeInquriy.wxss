page {
  background: #f5f5f5;
  box-sizing: border-box;
  padding-top: 1px;
  padding-bottom: 140rpx;
  /* height:100%; */
}

.type {
  box-sizing: border-box;
  font-size: 14px;
  width: 100%;
  /* height:76rpx; */
  height: 104rpx;
  background: #fff;
  border-radius: 2px;
  display: flex;
  align-items: center;
  padding-left: 28rpx;
  margin-top: 20rpx;
}

.inp {
  flex: 1;
  font-weight: 600;
  font-size: 16px;
}

.dec {
  margin-right: 30rpx;
  font-size: 16px;
  font-weight: 600;
}

/* 添加查询配件 */

.addPj {
  width: 100%;
  height: 90rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 1px;
  margin: 20rpx 0;
}

.qualityRange {
  height: 84rpx;
  width: 100%;
  box-sizing: border-box;
  font-size: 12px;
  background: #fff;
  font-weight: 600;
  line-height: 84rpx;
  margin-bottom: 1px;
  padding-left: 28rpx;
}

.color {
  color: #999;
}

/* 品质范围选项 */

.checkboxView {
  background: #fff;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 14px;
  color: #333;
  padding:0 28rpx;
  box-sizing: border-box;
  white-space: nowrap;
  overflow:hidden;
}

.checkboxView .group {
  height: 100rpx;
  white-space: nowrap;
  display: flex;
  justify-content: space-around;
}

/* checkbox 默认样式重置 */
checkbox .wx-checkbox-input {
  /* border-radius: 50%; */
  width: 30rpx;
  height: 30rpx;
  margin-top: -4rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 14rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

/* 配件列表  */

.top {
  height: 80rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 14px;
  font-weight: 600;
  padding-left: 28rpx;
  margin-bottom: 2rpx;
}

.number {
  width: 34rpx;
  height: 34rpx;
  text-align: center;
  line-height: 34rpx;
  border-radius: 50%;
  color: #f05b29;
  border: 1px solid #f05b29;
  margin-right: 16rpx;
}

/* 配件数量 */

.accessoriesInp {
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-left: 1px solid #f5f5f5;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 1px 0px 0px rgba(245, 245, 245, 1);
}

/* 配件名称 */

.name {
  font-size: 16px;
  font-weight: 600;
}

/* 删除按钮 */

.del {
  color: #999;
  font-size: 20px;
}

.remarks {
  /* height:140rpx; */
  height: 90rpx;
  width: 100%;
  font-size: 14px;
  color: #333;
  font-weight: 600;
  background: #fff;
  padding: 20rpx 28rpx;
  box-sizing: border-box;
  overflow: auto;
}

/* 图片上传组 */

.uploadGroup {
  background: #fff;
  padding: 20rpx;
}

.camera {
  width: 26px;
  height: 26px;
  margin-bottom: 14rpx;
}

.uploadImg {
  width: 160rpx;
  height: 160rpx;
  background: #f5f5f5;
  border: 1px dashed #c8c8c8;
  font-size: 14px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  /* text-align:center; */
}

/* 添加配件 */

.addPj {
  color: #f05b29;
  font-size: 10px;
  display: flex;
  justify-content: center;
  align-items: center;
  /* box-sizing:border-box; */
}

.addPj image{
  width: 44rpx;
  height: 44rpx;
  margin-right: 10rpx;
}

.addIcon {
  font-size: 30px;
  font-weight: 800;
  border: 1px dashed #f05b29;
  height: 100rpx;
  width: 100rpx;
  line-height: 80rpx;
  text-align: center;
  margin-bottom: 20rpx;
}

/* 发布询价 */

.savaBox {
  background: #fff;
  width: 100%;
  height: 140rpx;
  display: flex;
  align-items: center;
  position:fixed;
  bottom:0;
}

.sava {
  background: #f05b29;
  border-radius: 2px;
  width: 694rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 15px;
  color: #fff;
  letter-spacing: 0.14px;
}

/* 添加照片 */

.photo_box {
  padding: 30rpx;
  background: #fff;
}

.camera {
  width: 150rpx;
  height: 150rpx;
  border: 1px dashed #ddd;
  background-color: #f5f5f5;
  margin: 0 20rpx 20rpx 0;
  float: left;
  background: #fff;
}

/*清除浮动代码*/

.clearfloat:after {
  display: block;
  clear: both;
  content: "";
  visibility: hidden;
  height: 0;
}

.clearfloat {
  zoom: 1;
}

.camera text {
  color: #666;
  width: 100%;
  top: 70%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.camera_img {
  width: 56rpx;
  height: 50rpx;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.photo_img {
  width: 100%;
}

.del {
  width: 32rpx;
  height: 32rpx;
  top: 0;
  left: 0;
  transform: translate(-40%, -40%);
  background: #333;
  border-radius: 50%;
}

.del_img {
  width: 16rpx;
  height: 16rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.addPart {
  font-size: 36rpx;
  font-weight: 800;
  display: inline-block;
}

.submitText {
  margin: 0 0 20rpx;
  font-size: 36rpx;
}

.tipsText {
  font-size: 28rpx;
}
