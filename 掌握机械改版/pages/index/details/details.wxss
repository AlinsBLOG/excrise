page {
    background-color: #f4f4f4;
    height: 100%;
    overflow-y: hidden;
}

.body {
    height: 100%;
}

.nothing {
    width: 100%;
    height: 98rpx;
}

canvas {
    width: 592rpx;
    height: 985rpx;
    margin: 20rpx auto 25rpx auto;
    opacity: 0;
    position: absolute;
    top: -1000%;
}

/* 绑定手机弹窗 */
.tieForm {
    width: 750rpx;
    height: 100%;
    z-index: 2;
}

.inputCase {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    border: 1px solid #ddd;
    margin: 20rpx 0;
    border-radius: 2px;
}

.inputCase input {
    width: 60%;
    height: 88rpx;
    line-height: 88rpx;
}

.code {
    width: 70%;
}

.code button {
    width: 24%;
    height: 88rpx;
    line-height: 88rpx;
    right: 36rpx;
    background-color: #44b510;
    border-radius: 4rpx;
    padding: 0;
}

.tieForm form {
    width: 590rpx;
    height: 494rpx;
    top: 45%;
    left: 50%;
    transform: translate(-50%, -50%);
    padding: 40rpx;
    box-sizing: border-box;
}

.back {
    width: 40rpx;
    height: 40rpx;
    top: 20rpx;
    right: 20rpx;
}

.subbtn::after {
    border: none;
}

.submit {
    width: 510rpx;
    height: 88rpx;
    line-height: 88rpx;
    bottom: 40rpx;
    right: 40rpx;
    border-radius: 2px;
}

button[type=primary] {
    background: #44b510;
}

.mask {
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.70);
    top: 0;
    left: 0;
}

/* 清浮动 */

.clearfix:after {
    content: "";
    height: 0;
    line-height: 0;
    display: block;
    visibility: hidden;
    clear: both
}

.clearfix {
    zoom: 1;
}

.goodsImg,
swiper {
    width: 100%;
    height: 750rpx;
    border-bottom: 1px solid #eee;
}

.dots {
    position: absolute;
    right: 20rpx;
    bottom: 15rpx;
    overflow: hidden;
}

.dot {
    float: left;
    width: 15rpx;
    height: 15rpx;
    border: 1px solid #fff;
    border-radius: 50%;
    margin-left: 15rpx;
}

.bannerAct {
    border: 1px solid #fd5f2a;
