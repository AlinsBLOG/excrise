.page,.container{
  display: block;
  width: 100%;
  height: 100%;
  background: #f9f9f9;
}

.cate-nav{
  position: fixed;
  top: 0;
  left: 0;
  height: 86rpx;
  width: 100%;
  z-index: 99;
}

.cate-nav-body{
  background: #fff;
  height: 84rpx;
  white-space: nowrap;   
  border-top: 1px solid rgba(0,0,0,.15);
  overflow: hidden;
}

.cate-nav .item{
  text-align: center;
  display: inline-block;
  height: 84rpx;
  min-width: 250rpx;
  padding: 0 15rpx;
}

.cate-nav .item .name{
  display: block;
  height: 84rpx;
  padding: 0 20rpx;
  line-height: 84rpx;
  color: #333;
  font-size: 30rpx;
  width: auto;
}

.cate-nav .item.active .name{
  color: #1CBCB4;
  border-bottom: 1px solid #1CBCB4;
}

.list{
  margin-top: 101rpx;
}

.serviceInfo{
  background-color: #fff;
  padding: 20rpx 30rpx;
  border-bottom:1px solid #ececec; 
}

.serviceInfo .service,.serviceCenter .service{
  font-size: 24rpx;
  background-color: #FFFCEA;
  color: #CCB58D;
  border: 1px solid #E5DEC1;
  padding: 3rpx 8rpx;
  margin-right: 14rpx;
}

.serviceCenter .service{
  display: inline-block;
  margin-bottom: 5px;
}

.serviceInfo .name,.serviceCenter .name{
  font-size: 29rpx;
  vertical-align: middle;
  margin-right: 14rpx;
}

.serviceInfo .level{
  display: inline-block;
  background-color: #FABC01;
  color: #fff;
  font-size: 24rpx;
  padding: 3rpx 8rpx;
  transform:scale(0.8);
}

.serviceCenter{
  display: flex;
  padding: 30rpx 30rpx;
  background-color: #fff;
}

.serviceBottom{
   margin-bottom: 23rpx;
   border-top: 1px solid #ececec;
   padding:35rpx 30rpx;
   background: #fff;

}

.serviceCenter .left{
  flex:2;
}

.serviceCenter .left .title{
  font-size:34rpx;
  font-weight: 600;
  line-height: 1.5;
  margin-bottom: 15rpx;
  display: block;
}

.serviceCenter .left .area,
.serviceCenter .left .date{
  display: block;
  font-size: 28rpx;
  font-weight: 600;
}

.serviceCenter .left .area{
  margin-bottom: 8rpx;
}

.serviceCenter .right{
  flex: 1;
  text-align: right;
}

.serviceCenter .right{
  font-weight: 600;
  font-size: 30rpx;
}

.right .amount{
  color: #8C8C8C;
  display: inline-block;
  margin-top: 20px;
  font-size: 12px;
}

.serviceBottom .actPay{
  text-align: right;
  font-size: 31rpx;
  font-weight: 600;
}

.orderState{
  text-align: right;
  margin-top: 30rpx;
}

.orderState .cancel{
  border: 1px solid #e3e3e3;
  padding: 10rpx 14rpx;
  margin-right: 20rpx;
  border-radius:2px; 
  display: inline-block;
}

.orderState .payNow{
  padding: 10rpx 14rpx;
  background: #1CBCB4;
  color: #fff;
  border-radius:2px; 
  display: inline-block;
}

.serviceTitle .state{
  float: right;
  font-size: 30rpx;
  font-weight: 500;
  color: #1CBCB4;
}

.copyRight{
  height: auto;
  padding-bottom: 25rpx;
}