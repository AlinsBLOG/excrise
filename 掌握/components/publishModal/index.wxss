.modal {
  background: #000;
  opacity: .6;
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  z-index: 9999
}

.content {
  position: fixed;
  top: 50%;
  left: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #fff;
  border-radius: 10rpx;
  width: 80%;
  transform: translate(-50%,-50%);
  padding: 50rpx 0 30rpx;
  z-index: 999999
}

.content text {
  display: block;
}

.message {
  color: #F05B29;
  font-size: 50rpx;
  font-weight: 600;
}

.late {
  color: #B4B4B4;
  font-size: 28rpx;
}

.number {
  color: #F05B29;
  font-size: 80rpx;
  font-weight: 600;
  position: absolute;
}

.know {
  background: #F05B29;
  color: #fff;
  font-size: 32rpx;
  padding: 20rpx 0;
  width: 40%;
  border-radius: 5px;
  text-align: center;
}

.btnGroups {
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 100%;
}

.back {
  color: #F05B29;
  border: 1px solid #F05B29;
  font-size: 32rpx;
  padding: 20rpx 0;
  border-radius: 5px;
  width: 40%;
  text-align: center;
}

.ksBox {
  width: 100%;
  height: 400rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.ksImg {
  width: 530rpx;
  /* height: 80rpx; */
}

.ksBox .number {
  position: absolute;
}

.ksBox image {
  position: absolute;
}









.before{
  text-align: center;
  width: 100%;
}

.before image{
  width: 150rpx;
  height: 150rpx;
}

.submitText {
  color: #333;
  font-size: 44rpx;
  display: block;
  font-weight: 600;
  margin: 30rpx 0;
}

.tipsText {
  color: #666;
  font-size: 30rpx;
  display: block;
}

.btnGroup {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  margin-top: 30rpx;
  line-height: 100rpx;
  border-top: 1px solid #e3e3e3;
}

.btnGroup text{
  flex: 1;
  font-size: 36rpx;
}

.btnGroup text:not(:first-child) {
  border-left: 1px solid #e3e3e3;
}

.seePrice {
  color: #999;
}

.backIndex {
  color: #F05B29;
}