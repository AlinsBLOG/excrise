/* 绑定手机弹窗 */

.tieForm {
    width: 750rpx;
    height: 100%;
    z-index: 2;
  }
  
  .inputCase {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    border: 1px solid #ddd;
    margin: 20rpx 0;
    border-radius: 2px;
  }
  
  .inputCase input {
    width: 60%;
    height: 88rpx;
    line-height: 88rpx;
  }
  
  .code {
    width: 70%;
  }
  
  .code button {
    width: 24%;
    height: 88rpx;
    line-height: 88rpx;
    right: 36rpx;
    background-color: #44b510;
    border-radius: 4rpx;
    padding: 0;
  }
  
  .tieForm form {
    width: 590rpx;
    height: 494rpx;
    top: 45%;
    left: 50%;
    transform: translate(-50%, -50%);
    padding: 40rpx;
    box-sizing: border-box;
  }
  
  .back {
    width: 40rpx;
    height: 40rpx;
    top: 20rpx;
    right: 20rpx;
}
  
  .subbtn::after {
    border: none;
  }
  
  .submit {
    width: 510rpx;
