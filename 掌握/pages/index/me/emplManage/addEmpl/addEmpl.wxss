page {
  background: #f5f5f5;
  padding: 0 28rpx;
  box-sizing: border-box;
}

.register {
  margin-top: 16rpx;
  font-size: 14px;
  width: 100%;
  height: 76rpx;
  background: #fff;
  border-radius: 2px;
  display: flex;
  align-items: center;
}

.inp {
  flex: 1;
}

.dec {
  padding-left: 10rpx;
  margin-right: 30rpx;
}

.comfirmBtn {
  width: 100%;
  height: 70rpx;
  background: #f05b29;
  border-radius: 4px;
  color: #fff;
  text-align: center;
  line-height: 70rpx;
  font-size: 14px;
  margin-top: 40rpx;
}
