/* pages/index/me/myOrder/pendingOrderDetail/pendingOrderDetail.wxss */

page {
    width: 100%;
    height: 100%;
    background-color: #eee;
}

/* 复制链接弹窗 */

.download {
    width: 750rpx;
    height: 100%;
    z-index: 2;
    top: 0;
    left: 0;
    overflow: hidden;
}

.IOSview {
    width: 590rpx;
    height: 494rpx;
    top: 45%;
    left: 50%;
    transform: translate(-50%, -50%);
    padding: 40rpx;
    box-sizing: border-box;
    z-index: 9999;
}

.back {
    width: 40rpx;
    height: 40rpx;
    top: 20rpx;
    right: 20rpx;
}

.subbtn::after {
    border: none;
}

.download .submit {
    width: 410rpx;
    height: 70rpx;
    line-height: 70rpx;
    top: 75%;
    left: 50%;
    transform: translate(-50%, -50%);
    border-radius: 4px;
}

.mask {
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.70);
    top: 0;
    left: 0;
}

.download button {
    background: #fff;
    border: 1px solid red;
}

.download text {
    left: 50%;
    transform: translateX(-50%);
    width: 100%;
}

.appLogo {
    width: 140rpx;
    height: 140rpx;
    border-radius: 100%;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    overflow: hidden;
}

/* 订单倒计时 */

.p_orderTime {
    height: 108rpx;
    /* line-height: 30rpx; */
    width: 100%;
    background: #3e3e3e;
    color: #fff;
    justify-content: center;
}

.desc {
    margin-bottom: 14rpx;
}

.p_addr_num {
    line-height: 30rpx;
}

.p_orderTime .p_time {
    color: #f05b29;
}

.daifahuo {
    justify-content: center;
}

/* 订单地址 */

.p_orderAddr {
    padding: 16rpx 30rpx;
    color: #333;
    background: #fff;
    border-bottom: 2rpx solid #eee;
    box-sizing: border-box;
}

/* 买家留言 */

.P_sellerMsg {
    border-bottom: 0;
    padding: 12rpx 30rpx;
    word-break: break-all;
    word-wrap: break-word;
    line-height: 30rpx;
}

/* 左边的缩略图 */

/* .suolie_img {
  width: 32rpx;
  height: 42rpx;
  margin-right: 40rpx; 
  flex:none;
} */

/*店铺信息  */

.storeMsg {
    width: 100%;
    height: 72rpx;
    padding: 0 28rpx;
    background: #fff;
    justify-content: space-between;
    box-sizing: border-box;
}

.storeAvatar {
    width: 40rpx;
    height: 40rpx;
}

.storeName {
    margin: 0 18rpx;
}

.goStore {
    width: 12rpx;
    height: 18rpx;
}

.playSataus {
    color: #f05b29;
}

.shopMsg {
    padding: 0 28rpx;
    background: #fafafa;
    justify-content: space-between;
}

.shopMsg .shopMsg_left {
    align-self: flex-start;
    padding-top: 12rpx;
    top: 20rpx;
}

.goodImg {
    width: 140rpx;
    height: 140rpx;
    border: 2rpx solid #eee;
    margin-right: 28rpx;
}

.prod_name {
    color: #666;
}

.shopMsg_right {
    text-align: right;
    top: 10rpx;
    right: 20rpx;
}

.goodDsc {
    width: 382rpx;
    align-self: flex-start;
}

.prod_title {
    line-height: 28rpx;
}

.oldPrice {
    color: #999;
    text-decoration: line-through;
}

.prod_num {
    color: #999;
}

.refund {
    font-size: 11px;
    color: #f2a841;
}

/* 商品价格 */

.shopPrice {
    height: 72rpx;
    line-height: 72rpx;
    width: 100%;
    text-align: right;
    color: #333;
    border-bottom: 2rpx solid #eee;
    margin-right: 28rpx;
}

.shopPriceNum {
    margin-right: 28rpx;
}

.P_allPrice {
    justify-content: space-between;
    padding: 0 20rpx;
    height: 108rpx;
    background: #fff;
    font-size: 12px;
    color: #666;
    box-sizing: border-box;
    border-bottom: 2rpx solid #eee;
}

.price_num {
    text-align: right;
    color: #333;
}

/*实付  */

.p_shifu {
    height: 80rpx;
    line-height: 80rpx;
    color: #333;
    padding: 0 20rpx;
    background: #fff;
    border-bottom: 2rpx solid #eee;
}

.p_shifu .price_num {
    color: #f05b29;
}

.playSataus .kefu {
    width: 24rpx;
    height: 26rpx;
    margin-right: 10rpx;
}

.p_kefu {
    height: 80rpx;
    line-height: 60rpx;
    color: #333;
    background: #fff;
    box-sizing: border-box;
    padding: 10rpx 0;
    margin-bottom: 20rpx;
}

.kefu_online,
.callPhone {
    box-sizing: border-box;
    width: 50%;
    text-align: center;
    height: 80rpx;
    justify-content: center;
    flex: 1;
}

.kefu_online {
    border-right: 2rpx solid #eee;
}

.p_kefu image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 18rpx;
    vertical-align: middle;
}

/*订单详情  */

.orderNumber {
    box-sizing: border-box;
    /* height: 110rpx; */
    padding: 24rpx 20rpx 20rpx 20rpx;
    background: #fff;
    font-size: 20rpx;
    color: #999;
    margin-bottom: 20rpx;
}

/* 左边 */

.orderNumber_l {
    margin: 10rpx 20rpx 10rpx 0;
}

/*在线客服  */

.concat_btn {
    font-size: 26rpx;
    border-radius: 0;
    background-color: #fff;
    flex: 1;
    justify-content: center;
}

.concat_btn::after {
    border: 0;
}

/*复制按钮  */

.copy {
    border-radius: 2rpx;
    width: 108rpx;
    height: 38rpx;
    line-height: 38rpx;
    font-size: 22rpx;
    color: #666;
    border: 2rpx solid #c8c8c8;
    right: 28rpx;
    top: 22rpx;
    text-align: center;
}

/* 支付凭证 */

.paymentVoucher {
    top: 120rpx;
}

.playSataus .concat_btn {
    padding: 0;
    color: #666;
}

/* 猜你喜欢 */

.rec_title {
    height: 112rpx;
    line-height: 112rpx;
    background: #fff;
    color: #666;
}

.waterfall_box {
    width: 744rpx;
    padding-bottom: 10rpx;
}

/* 循环子盒子宽高 */

.waterfall_goods {
    width: 366rpx;
    height: 584rpx;
    margin-right: 7rpx;
    padding-bottom: 10rpx;
    float: left;
}

/* 待收货的title样式 */

.pendingDelivery .p_OrderMsg {
    padding: 0;
}

.pendingDelivery .storeMsg {
    padding-left: 20rpx;
}

.pendingDelivery .title {
    line-height: 108rpx;
}

.pendingDelivery .shopMsg {
    background: #fafafa;
    width: 100%;
    height: 222rpx;
    padding: 10rpx 20rpx 0 20rpx;
    /* box-sizing: border-box; */
    align-items: flex-start;
    justify-content: space-between;
}

.pendingDelivery .shopTitle {
    width: 382rpx;
}

.pendingDelivery .shopImg {
    width: 140rpx;
    height: 140rpx;
}

.pendingDelivery .shopSize {
    padding-bottom: 10rpx;
}

.pendingDelivery .shop_price {
    text-align: right;
    color: #666;
}

.old_price {
    color: #999;
    text-decoration: line-through;
    margin-top: 4rpx;
}

/* 退款按钮 */

.tuikuan_btn {
    background: #fff;
    border: 2rpx solid #999;
    border-radius: 200rpx;
    /* width:136rpx; */
    height: 50rpx;
    line-height: 50rpx;
    color: #999999;
    margin-top: 42rpx;
    text-align: center;
}

/* 订单编号 */

.orderList {
    padding: 24rpx 20rpx 20rpx 20rpx;
}

.Logistics {
    color: #f05b29;
    margin-bottom: 4rpx;
}

.pendingReceived .LogisticsBox {
    width: 279px;
}

.pendingReceived .more {
    width: 14rpx;
    height: 22rpx;
    top: 50%;
    right: 30rpx;
    transform: translateY(-50%);
}

.pendingReceived .logisticsImg {
    width: 38rpx;
    height: 36rpx;
    margin-right: 34rpx;
}

/* 贷收货 */

.paymentVoucher {
    top: 116rpx;
}

/* 待付款 */

.shopPlay {
    background: #fff;
    padding-right: 28rpx;
    height: 88rpx;
    border-top: 1px solid #eee;
    justify-content: flex-end;
    background: #fff;
    position: fixed;
    bottom: 0;
    right: 0;
    width: 100%;
    z-index: 100;
}

.shopPlay view {
    width: 136rpx;
    height: 48rpx;
    text-align: center;
    line-height: 48rpx;
    margin-left: 20rpx;
    border-radius: 100px;
    border: 2rpx solid #c8c8c8;
}

.shopPlay .fukuan {
    color: #f05b29;
    border-radius: 100px;
    border: 2rpx solid #f05b29;
}

/* 取消订单 */

/*使屏幕变暗  */

.mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.6;
    overflow: hidden;
    z-index: 1000;
}

/*对话框 */

.toast_cont {
    height: 760rpx;
    width: 100%;
    overflow: hidden;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2000;
    background: #fff;
    padding-top: 30rpx;
    color: #333;
    font-size: 28rpx;
}

.modalBox {
    padding-left: 20rpx;
}

/* 模态框标题 */

.modalTitle {
    height: 126rpx;
    text-align: center;
}

.formData view {
    height: 86rpx;
    line-height: 86rpx;
    border-bottom: 2rpx solid #e6e6e6;
}

/* 取消按钮 */

.cancelBtn {
    bottom: 0;
    left: 0;
    background: #f05b29;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    color: #fff;
    border-radius: 0;
}

/* 退款状态 */

.refundableState {
    background: #fff;
    height: 48rpx;
    width: 136rpx;
    margin-bottom: 20rpx;
    margin-top: 28rpx;
    border-radius: 200rpx;
}

.backStatus {
    background: #fafafa;
    padding: 0 28rpx 26rpx 28rpx;
}

.btn {
    background: #fff;
    border: 2rpx solid #999;
    border-radius: 100px;
    height: 48rpx;
    line-height: 48rpx;
    padding: 0 32rpx;
}

.clearfix::after {
    content: "";
    display: block;
    clear: both;
}

.shopBox {
    background: #fff;
}

.shopBox view:not(:last-child) {
    margin-bottom: 4rpx;
}

.icon {
    margin-right: 34rpx;
    font-size: 42rpx;
    color: #666;
}

.deliverIcon {
    transform: rotateY(180deg);
}

/* 申请退款弹框 */

.shouhou {
    background: #fff;
    text-align: center;
    width: 600rpx;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    z-index: 9999;
}

.shouhou .title {
    width: 100%;
    height: 150rpx;
    line-height: 150rpx;
    border-bottom: 2rpx solid #eee;
    color: #666;
}

.comfirmBtn {
    color: #ff0017;
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
}

.mask {
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.70);
    top: 0;
    left: 0;
    z-index: 1000;
}

.mj_message {
    color: #333;
    padding: 20rpx;
    background: #fff;
    border-bottom: 2rpx solid #eee;
    justify-content: space-between;
}

