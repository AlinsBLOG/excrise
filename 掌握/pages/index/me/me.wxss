page {
  height: 100%;
  background: #f4f4f4;
}

.badge {
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: white;
  top: -16%;
  right: 5%;
  border-radius: 50%;
  border: 3rpx solid #fd5f2a;
  color: #fd5f2a;
}

#inquiry_badge {
  top: -20%;
  right: 28%;
}

.otherIcon {
  width: 32rpx;
  height: 32rpx;
  top: 50%;
  transform: translateY(-50%);
  left: 5%;
}

.phoneIcon {
  width: 32rpx;
  height: 45rpx;
  top: 50%;
  transform: translateY(-50%);
  left: 5%;
}

/* 个人头像 */

.portrait {
  widows: 100%;
  height: 282rpx;
  background: #1e1e1e;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 30rpx;
}

.portrait_img {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  overflow: hidden;
}

.portrait_img_right {
  flex: 1;
  margin-left: 30rpx;
}

.login {
  width: 180rpx;
  height: 100%;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
  padding: 0;
}

.login image {
  width: 28rpx;
  height: 44rpx;
  top: 50%;
  transform: translate(-50%, -50%);
  left: 50%;
}

/* 我的订单*/

.myorder {
  background: #fff;
  width: 100%;
  /* height: 202rpx; */
  font-size: 14px;
  color: #333;
}

.myorder .title {
  height: 96rpx;
  line-height: 96rpx;
  width: 100%;
  border-bottom: 2rpx solid #eee;
}

.order_me {
  font-size: 28rpx;
  color: #333;
  margin-left: 26rpx;
}

.myorder .more {
  width: 14rpx;
  height: 22rpx;
  margin: 0 26rpx 0 10rpx;
}

.orderStatus .payIcon {
  font-size: 44rpx;
  margin-bottom: 10rpx;
}

.myorder .orderStatus {
  height: 120rpx;
  width: 100%;
  justify-content: space-around;
}

.tie {
  width: 100rpx;
  height: 44rpx;
  border: 1px solid #ff0017;
  text-align: center;
  line-height: 44rpx;
  color: red;
}

/* 绑定手机弹窗 */

.tieForm {
  width: 750rpx;
  height: 100%;
  z-index: 2;
}

.inputCase {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  border: 1px solid #ddd;
  margin: 20rpx 0;
  border-radius: 2px;
}

.inputCase input {
  width: 60%;
  height: 88rpx;
  line-height: 88rpx;
}

.code {
  width: 70%;
}

.code button {
  width: 24%;
  height: 88rpx;
  line-height: 88rpx;
  right: 36rpx;
  background-color: #44b510;
  border-radius: 4rpx;
  padding: 0;
}

.tieForm form {
  width: 590rpx;
  height: 494rpx;
  top: 45%;
  left: 50%;
  transform: translate(-50%, -50%);
  padding: 40rpx;
  box-sizing: border-box;
}

.back {
  width: 40rpx;
  height: 40rpx;
  top: 20rpx;
  right: 20rpx;
}

.subbtn::after {
  border: none;
}

.submit {
  width: 510rpx;
  height: 88rpx;
  line-height: 88rpx;
  bottom: 40rpx;
  right: 40rpx;
  border-radius: 2px;
}

button[type=primary] {
  background: #44b510;
}

/* -------------------------------------- */

.list {
  background: #fff;
  width: 100%;
  height: 104rpx;
  border-bottom: 1px solid #d9d9d9;
  box-sizing: border-box;
}

.list>text {
  top: 50%;
  transform: translateY(-50%);
  left: 12%;
}

.service::after {
  border: none;
}

.share {
  background: rgba(0, 0, 0, 0.70);
  width: 96rpx;
  height: 96rpx;
  bottom: 5%;
  right: 2%;
  border-radius: 100%;
  box-shadow: 0 2rpx 2rpx rgba(0, 0, 0, 0.7);
  color: #bbb;
}

.share image {
  width: 46rpx;
  height: 46rpx;
  top: 16rpx;
  left: 50%;
  transform: translateX(-50%);
}

.share text {
  bottom: 10rpx;
  left: 50%;
  transform: translateX(-50%);
}

.share::after {
  border: none;
}

/*遮罩层部分  */

.shouhou {
  background: #fff;
  text-align: center;
  width: 600rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.shouhou .title {
  width: 100%;
  height: 150rpx;
  line-height: 150rpx;
  border-bottom: 2rpx solid #eee;
  color: #666;
}

.comfirmBtn {
  color: #ff0017;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
}

.more {
  width: 14rpx;
  height: 22rpx;
}

.favorBox {
  padding: 0 26rpx 0 30rpx;
  justify-content: space-between;
}

.favorBox .favorIcon {
  margin-right: 18rpx;
  color: #333;
  font-size: 40rpx;
  vertical-align: middle;
}

/* 复制链接弹窗 */

.download {
  width: 750rpx;
  height: 100%;
  z-index: 2;
  top: 0;
  left: 0;
  overflow: hidden;
}

.IOSview {
  width: 590rpx;
  height: 494rpx;
  top: 45%;
  left: 50%;
  transform: translate(-50%, -50%);
  padding: 40rpx;
  box-sizing: border-box;
  z-index: 9999;
}

.back {
  width: 40rpx;
  height: 40rpx;
  top: 20rpx;
  right: 20rpx;
}

.subbtn::after {
  border: none;
}

.download .submit {
  width: 410rpx;
  height: 70rpx;
  line-height: 70rpx;
  top: 75%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 4px;
}

.mask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.70);
  top: 0;
  left: 0;
}

.download button {
  background: #fff;
  border: 1px solid red;
}

.download text {
  left: 50%;
  transform: translateX(-50%);
  width: 100%;
}

.appLogo {
  width: 140rpx;
  height: 140rpx;
  border-radius: 100%;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
}

/* 求购系统 2018.9.8 -bobo */

