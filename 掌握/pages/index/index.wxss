page {
  background-color: #eee;
  height: 100%;
}

.price {
  color: #fd5f2a;
}

.container {
  height: calc(100% - 54rpx);
  padding-top: 75rpx;
}

.search {
  /* width: 100%; */
  width: 98%;
  height: 54rpx;
  padding: 10rpx;
  align-items: center;
  z-index: 2;
}

.searchIcon {
  width: 32rpx;
  height: 32rpx;
  left: 45%;
  top: 50%;
  margin-left: -91px;
  transform: translateY(-50%);
}

.messageImg {
  width: 32rpx;
  height: 32rpx;
}

.search input {
  /* background-color: #e6e6e6; */
  border: 1px solid #e6e6e6;
  border-radius: 100px;
}

/*banner*/
.swiper, swiper {
  width: 100%;
  /* height: 375rpx; */
  position: relative;
  margin-bottom: 20rpx;
}

.dots {
  position: absolute;
  right: 20rpx;
  bottom: 15rpx;
  overflow: hidden;
}

.dot {
  float: left;
  width: 15rpx;
  height: 15rpx;
  border: 1px solid #fff;
  border-radius: 50%;
  margin-left: 15rpx;
}

.bannerAct {
  border: 1px solid #fd5f2a;
  background-color: #fd5f2a;
}

/*server*/
.server {
  height: 75rpx;
}

.server_item {
  height: 75rpx;
  flex: 1;
  text-align: center;
  font-size: 10px;
  color: #acacac;
  line-height: 75rpx;
  vertical-align: middle;
}

.p {
  width: 100%;
  text-align: center;
  font-size: 12px;
  color: #aaa;
}

.p image {
  width: 28rpx;
  height: 28rpx;
}

/*prod*/

.prod_class {
  width: 100%;
  height: 340rpx;
  overflow: hidden;
  font-weight: 600;
}

.prod_item {
  float: left;
  width: 20%;
  height: 150rpx;
}

.prod_image {
  width: 85rpx;
  height: 85rpx;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
}

/*refinement*/

.refinement {
  width: 100%;
  height: 608rpx;
  padding: 25rpx;
  padding-top: 0;
}

.ref_title {
  height: 96rpx;
  line-height: 96rpx;
}

.ref_box {
  width: 100%;
  height: 492rpx;
  overflow: hidden;
}

.ref_l, .ref_r {
  width: 349rpx;
  height: 492rpx;
}

.ref_lt, .ref_lb {
  width: 349rpx;
  height: 244rpx;
}

.ref_lb {
  margin-top: 8rpx;
}

/*recommend*/

.rec_title {
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
}

.rec_goods_label {
  font-weight: 600;
}

.rec_title image {
  width: 9px;
  height: 15px;
  top: 50%;
  margin-top: -7px;
  left: 50%;
  margin-left: 35px;
}

.rec_goodsbox {
  width: 100%;
  height: 320rpx;
  padding: 30rpx 0;
  border-bottom: 1px solid #eee;
}

.rec_image {
  width: 260rpx;
  height: 260rpx;
  margin-right: 40rpx;
}

.rec_goodsinfo {
  width: 420rpx;
  height: 260rpx;
}

.rec_goodsname {
  height: 72rpx;
  line-height: 36rpx;
  font-weight: 600;
  font-size: 30rpx;
}

.rec_company {
  width: 100%;
  height: 32rpx;
  line-height: 32rpx;
}

.rec_goodsprice {
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 25rpx;
}

/*supply*/

.rec_title .supply_img {
  margin-left: 60px;
}

.supply_box {
  width: 100%;
  height: 470rpx;
  padding: 0 15rpx;
}

.supply_line {
  flex-flow: row nowrap;
  justify-content: space-around;
}

.supply_goods {
  width: 355rpx;
  height: 225rpx;
  padding: 0 4rpx 4rpx 0;
}

/*special*/

.rec_title .special_img {
  margin-left: 20px;
}

.special {
  width: 100%;
  height: 448rpx;
}

.special_scroll {
  width: 100%;
  height: 448rpx;
  white-space: nowrap;
}

.special_box {
  width: 660rpx;
  height: 448rpx;
  display: inline-block;
  margin-right: 30rpx;
}

.special_banner {
  width: 660rpx;
  height: 320rpx;
  border-radius: 3px;
  overflow: hidden;
}

.special {
  white-space: nowrap;
}

.scroll-view-item_H {
  width: 100%;
  display: inline-block;
}

.scroll-view-item_H {
  height: 200rpx;
  border: 1px solid #f00;
}

