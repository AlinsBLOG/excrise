.returnTop {
  bottom: 70rpx;
  right: 3%;
  width: 68rpx;
  height: 68rpx;
  border: 1px solid #ccc;
  border-radius: 50%;
  box-shadow: 0 2rpx 2rpx rgba(0, 0, 0, 0.7);
  text-align: center;
  opacity: 0.8;
}

.return_img {
  width: 40rpx;
  height: 20rpx;
  margin: 14rpx auto 0;
}

.share {
  background: rgba(0, 0, 0, 0.70);
  width: 96rpx;
  height: 96rpx;
  bottom: 15%;
  right: 2%;
  border-radius: 100%;
  box-shadow: 0 2rpx 2rpx rgba(0, 0, 0, 0.7);
  color: #bbb;
}

.share image {
  width: 46rpx;
  height: 46rpx;
  top: 16rpx;
  left: 50%;
  transform: translateX(-50%);
}

.share text {
  bottom: 10rpx;
  left: 50%;
  transform: translateX(-50%);
}

.share::after {
  border: none;
}

.f8 {
  font-size: 8px;
}

/*新增的求购信息  */

.swiperBox {
  background: #fff;
  padding: 0 25rpx;
}

.swiperBox .qg_swiper {
  background: #fff;
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
}

.qg_title {
  font-size: 20px;
  color: #333;
  width: 120rpx;
}

.qg_line {
  background: #dcdcdc;
  width: 2rpx;
  height: 40rpx;
  left: 154rpx;
  top: 50%;
  transform: translateY(-50%);
}

.qg_subTittle {
  font-size: 10px;
  color: #333;
}

.qg_info {
  padding-top: 10rpx;
  font-size: 26rpx;
  color: #333;
  line-height: 35rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.qg_status {
  font-size: 24rpx;
  color: #b4b4b4;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
}

.qg_item {
  width: 100%;
  height: 76rpx;
  padding-right: 200rpx;
}

.qg_success {
  color: green;
  border: 2rpx solid green;
}

.qg_xunjia {
  color: #f5a623;
  border: 2rpx solid #f5a623;
}

.trade_logo {
  width: 149rpx;
  height: 36rpx;
  margin-right: 38rpx;
}

.qg_btn {
  background: skyblue;
  font-size: 30rpx;
  line-height: 40rpx;
  height: 40rpx;
  padding: 10rpx 20rpx;
  text-align: center;
  border-radius: 20rpx;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
}

