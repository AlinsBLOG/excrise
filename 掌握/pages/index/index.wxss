/* 询价系统 */

.swiperBox .inquery_swiper {
  background: #fff;
  width: 100%;
  height: 72rpx;
  /* box-sizing: border-box;   */
  margin-bottom: 2rpx;
}

.inqueryBox {
  width: 580rpx;
  margin-right: 20rpx;
}

.inqueryItem {
  display: flex;
  width: 100%;
  justify-content: space-between;
  align-items: center;
  font-weight: 600;
  color: #999;
  font-size: 24rpx;
  height: 72rpx;
  background: #fff;
  border-bottom: 1px solid #f5f5f5;
  border-radius: 16rpx 16rpx 0 0;
  padding: 0 28rpx;
  box-sizing: border-box;
}

.inquery_status {
  color: #fff;
  padding: 0 4px;
}

/* 询价大厅入口 */

.inquery_root {
  width: 100%;
  height: 190rpx;
  background: #fff;
  border-radius: 0 0 16rpx 16rpx;
}

.inqueryBox .inquery_num {
  color: #999;
}

.inquery_img {
  width: 130rpx;
  height: 120rpx;
  margin-right: 18rpx;
  margin-left: 28rpx;
}

.inquery_root .title {
  font-weight: 600;
  font-size: 16px;
  width: 150px;
  overflow: hidden;
  text-overflow: ellipsis;
}

.inquery_root .info {
  font-size: 14px;
  font-weight: 600;
  color: #666;
}

/* 颜色状态 */

/*
  *  yellow 待报价 
  */

.yellow {
  background: #f5a623;
}

.mask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.70);
  top: 0;
  left: 0;
  z-index: 9999;
}
